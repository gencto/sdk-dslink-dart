{
  "name": "test-link_provider",
  "version": "0.0.1",
  "description": "An example dsalink to illustrate the usage of a dart dsalink",
  "author": {
    "name": "Rick Zhou",
    "email": "z.zhou@dglogik.com"
  },
  "contributors": [
    {
      "name": "Kenneth Endfinger",
      "email": "kaendfinger@gmail.com"
    }
  ],
  "main": "test_link_provider.dart",
  "engines": {
    "dart": ">=1.9.0"
  },
  "getDependencies": [
    "pub get"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/IOT-DSA/sdk-dsalink-dart"
  },
  "bugs": {
    "url": "https://github.com/IOT-DSA/sdk-dsalink-dart/issues"
  },
  "keywords": [
    "test"
  ],
  "configs": {
    "broker": {
      "type": "url",
      "required": false
    },
    "nodes": {
      "type": "path",
      "value": "nodes.json",
      "required": true
    },
    "key": {
      "type": "path",
      "value": ".dsalink.key",
      "required": true
    }
  },
  "license": "Apache"
}